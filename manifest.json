{
  "name": "Tab Manager",
  "manifest_version": 3,
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "minimum_chrome_version": "88"
}