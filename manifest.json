{
  "name": "Browser Hero",
  "description": "A tab manager that sleeps tabs and exports tabs to be used later",
  "manifest_version": 3,
  "version": "0.1.0",
  "icons": {
    "16": "./assets/icons/icon16.png",
    "48": "./assets/icons/icon48.png",
    "128": "./assets/icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Browser Hero"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "minimum_chrome_version": "88"
}